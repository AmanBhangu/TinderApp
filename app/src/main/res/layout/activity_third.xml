<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.taranjotsidhu.tinder.Third">


    <LinearLayout
        android:layout_width="395dp"
        android:layout_height="643dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:orientation="vertical"
        android:weightSum="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="56dp"
                android:layout_height="54dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.01"
                app:srcCompat="@drawable/fb" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="80dp"
                android:layout_height="55dp"
                android:layout_marginBottom="10dp"
                android:layout_weight="0.23"
                android:adjustViewBounds="false"
                app:srcCompat="@drawable/fc" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="66dp"
                android:layout_height="58dp"
                app:srcCompat="@drawable/fa"
                android:onClick="image"/>

        </LinearLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="520dp">

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@drawable/dala"
                android:onClick="mes"/>

            <TextView
                android:id="@+id/textView12"
                android:layout_width="161dp"
                android:layout_height="50sp"
                android:layout_gravity="bottom"
                android:text="Sara, 22"
                android:textAppearance="@style/TextAppearance.AppCompat.Body1"
                android:textColor="#FFFFFF"
                android:textSize="25sp" />
        </FrameLayout>

        <LinearLayout
            android:layout_width="404dp"
            android:layout_height="68dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="66dp"
                android:layout_height="match_parent"
                android:layout_marginRight="15dp"
                app:srcCompat="@drawable/fd" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="66dp"
                android:layout_height="match_parent"
                android:layout_marginRight="15dp"
                android:layout_weight="0.02"
                app:srcCompat="@drawable/tb" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="66dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                app:srcCompat="@drawable/fe" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="66dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                app:srcCompat="@drawable/td" />

            <ImageView
                android:id="@+id/imageView21"
                android:layout_width="66dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                app:srcCompat="@drawable/tc" />
        </LinearLayout>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
