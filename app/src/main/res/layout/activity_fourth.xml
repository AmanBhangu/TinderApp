<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.taranjotsidhu.tinder.Fourth">


    <LinearLayout
        android:layout_width="784dp"
        android:layout_height="1191dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="14dp"
        android:orientation="vertical"
        android:weightSum="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.375">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="122dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <ImageButton
                android:id="@+id/imageButton11"
                android:layout_width="98dp"
                android:layout_height="98dp"
                android:layout_weight="0.04"
                app:srcCompat="@drawable/back" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="98dp"
                android:layout_height="98dp"
                android:layout_marginLeft="115dp"

                app:srcCompat="@drawable/aa" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="60sp"
                android:layout_height="77dp"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
                android:layout_weight="0.20"
                android:text="Alithia"
                android:textSize="35dp"
                 />

        </LinearLayout>

        <TextView
            android:id="@+id/textView22"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:textSize="25dp"
            tools:text="                        YOU MATCHED WITH ALITHIA ON 2017-05-03" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
